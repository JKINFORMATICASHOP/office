# Defina o diretório que você deseja listar
$diretorio = "https://raw.githubusercontent.com/JKINFORMATICASHOP/ativador/master/MAS/Separate-Files-Version/Activators/HWID_Activation.cmd"

# Defina o caminho do arquivo de saída
$arquivoSaida = "C:\Seu\Caminho\Aqui\lista_arquivos.txt"

# Liste os arquivos e salve no arquivo de saída
Get-ChildItem -Path $diretorio | Select-Object FullName | Out-File -FilePath $arquivoSaida

# Mensagem de conclusão
Write-Host "A lista de arquivos foi salva em $arquivoSaida"
